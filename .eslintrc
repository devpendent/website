{
  "extends": ["standard", "standard-react"],
  "parser": "babel-eslint",
  "globals": {
    "describe": true,
    "it": true
  },
  "plugins": ["eslint-plugin-cypress"],
  "rules": {
    "import/first": "off",
    "import/export": "off",
    "function-paren-newline": ["error", "consistent"],
    "no-console": "error",
    "react/jsx-curly-brace-presence": ["error", "never"]
  },
  "env": {
    "jest": true,
    "cypress/globals": true
  }
}
