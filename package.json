{
  "name": "@devpendent/website",
  "private": false,
  "description": "An Open Sourced Platform for Indonesia Election Real Count",
  "version": "0.1.0",
  "author": "Zain Fathoni <zain.fathoni@gmail.com>",
  "dependencies": {
    "@emotion/core": "10.0.10",
    "@fortawesome/fontawesome-svg-core": "1.2.17",
    "@fortawesome/free-solid-svg-icons": "5.8.1",
    "@fortawesome/react-fontawesome": "0.1.4",
    "font-awesome": "4.7.0",
    "gatsby": "2.4.1",
    "gatsby-image": "2.0.41",
    "gatsby-plugin-antd": "2.0.2",
    "gatsby-plugin-emotion": "4.0.6",
    "gatsby-plugin-less": "2.0.12",
    "gatsby-plugin-manifest": "2.1.1",
    "gatsby-plugin-offline": "2.1.0",
    "gatsby-plugin-react-helmet": "3.0.12",
    "gatsby-plugin-sharp": "2.0.36",
    "gatsby-source-filesystem": "2.0.33",
    "gatsby-transformer-sharp": "2.1.19",
    "less": "3.9.0",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1"
  },
  "devDependencies": {
    "@snowcoders/sortier": "2.6.1",
    "babel-jest": "24.7.1",
    "babel-preset-gatsby": "0.1.11",
    "cross-env": "5.2.0",
    "cypress": "3.2.0",
    "cypress-testing-library": "3.0.1",
    "eslint": "5.16.0",
    "eslint-config-standard": "12.0.0",
    "eslint-config-standard-react": "7.0.2",
    "eslint-plugin-cypress": "2.2.1",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-node": "9.0.0",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-react": "7.13.0",
    "eslint-plugin-standard": "4.0.0",
    "husky": "2.2.0",
    "identity-obj-proxy": "3.0.0",
    "is-ci-cli": "1.1.1",
    "jest": "24.7.1",
    "lint-staged": "8.1.5",
    "npm-run-all": "4.1.5",
    "prettier-eslint-cli": "4.7.1",
    "react-testing-library": "7.0.0",
    "start-server-and-test": "1.9.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "src/**/*.{js,json}": [
        "prettier-eslint --write",
        "npx sortier",
        "git add",
        "jest --findRelatedTests"
      ],
      "src/**/*.json": [
        "prettier-eslint --write",
        "npx sortier",
        "git add"
      ],
      "gatsby-*.js": [
        "prettier-eslint --write",
        "git add",
        "jest --findRelatedTests"
      ]
    },
    "ignore": [
      "**/package.json",
      "**/yarn.lock"
    ]
  },
  "keywords": [
    "devpendent",
    "election",
    "indonesia"
  ],
  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop",
    "start": "yarn develop",
    "format": "prettier-eslint --write \"{src/**/*,gatsby-*}.{js,json}\"",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "serve:e2e": "gatsby serve -p 8000",
    "test": "jest --watch",
    "test:coverage": "jest --coverage",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:ci": "cypress run --record --key $CYPRESS_DASHBOARD_KEY",
    "test:e2e:dev": "start-server-and-test start http://localhost:8000 cy:open",
    "pretest:e2e:run": "yarn build",
    "test:e2e:run": "start-server-and-test serve:e2e http://localhost:8000 cy:run",
    "pretest:e2e:run:ci": "yarn build",
    "test:e2e:run:ci": "start-server-and-test serve:e2e http://localhost:8000 cy:run:ci",
    "test:e2e": "is-ci \"test:e2e:run:ci\" \"test:e2e:dev\"",
    "validate": "cross-env CI=true npm-run-all --parallel test:coverage test:e2e"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devpendent/website"
  },
  "bugs": {
    "url": "https://github.com/devpendent/website/issues"
  }
}
